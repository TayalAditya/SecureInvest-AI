{
  "name": "secureinvest-ai",
  "version": "1.0.0",
  "description": "Comprehensive fraud prevention platform for securities market",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "fraud-detection",
    "securities-market",
    "ai-ml",
    "investor-protection",
    "sebi",
    "fintech"
  ],
  "author": "Aditya Tayal <adityatayal404@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.5.0",
    "socket.io": "^4.7.2",
    "redis": "^4.6.8",
    "node-cron": "^3.0.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "nodemailer": "^6.9.4",
    "twilio": "^4.15.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
